			Snackbar.show({
				text: 'Your status has been changed!',
				pos: 'bottom-center',
				showAction: false,
				actionText: "Dismiss",
				duration: 3000,
				textColor: '#fff',
				backgroundColor: '#383838'
			});
		});

		// Snackbar for "place a bid" button
		$('#snackbar-place-bid').click(function () {
			Snackbar.show({
				text: 'Your bid has been placed!',
			});
		});


