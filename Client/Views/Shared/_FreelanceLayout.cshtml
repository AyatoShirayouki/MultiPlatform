    <script type="text/javascript">
        $(document).ready(function () {
			$.ajax({
                type: 'GET',
				url: '/Authentication/LoadAllCountries',
                dataType: 'json',
			    data: null,
                success: function (countries) {
					let index;
					$.each(countries, function (i, country) {
						index = i + 1;
						$('#selectCountriesOuther').find('ul').append('<li data-original-index="' + index + '"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">' + country.name + '</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
						$('#countriesList').append('<option value="' + country.id + '">' + country.name + '</option>');
					});
                },
                error: function (ex) {
                    console.log(ex)
                }
            });

			function loadRegions() {
				$('#regionsList').empty();

				$('#selectRegionsOuther').find('ul').empty();
				$('#selectRegionsOuther').find('.filter-option').text('Nothing selected');
				$('#selectRegionsOuther').find('button').attr('title', 'Nothing selected');

				$('#citiesList').empty();

				$('#selectCitiesOuther').find('ul').empty();
				$('#selectCitiesOuther').find('.filter-option').text('Nothing selected');
				$('#selectCitiesOuther').find('button').attr('title', 'Nothing selected');
				$.ajax({
					type: 'GET',
					url: '/Authentication/LoadRegionsForCountry',
					dataType: 'json',
					data: { id: $('select[name="CountryId"]').val() },
					success: function (regions) {
						let other;
						let IDs;
						let index;
						if (regions.length == 0) {
							$('#selectRegionsOuther').find('ul').append('<li data-original-index="0" class="selected"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">Nothing selected</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
							$('#regionsList').append('<option value="0">Nothing selected</option>');

							$('#selectCitiesOuther').find('ul').append('<li data-original-index="0" class="selected"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">Nothing selected</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
							$("#citiesList").append('<option value="0">Nothing selected</option>');
						}
						else if (regions.length > 1) {
							$('#selectRegionsOuther').find('ul').append('<li data-original-index="0" class="selected"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">Nothing selected</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
							$('#regionsList').append('<option value="0">Nothing selected</option>');

							$.each(regions, function (i, region) {
								index = i + 1;
								if (region.name.includes("Other") == false) {
									$('#selectRegionsOuther').find('ul').append('<li data-original-index="' + index + '"><a tabindex="0" class="" data-tokens="null" role="option" ari-disabled="false" aria-selected="false"><span class="text">' + region.name + '</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
									$('#regionsList').append('<option value="' + region.id + '">' + region.name + '</option>');
								}
								else {
									other = region.name.split("-")[0];
									IDs = region.name.split("-")[1];

									$('#selectRegionsOuther').find('ul').append('<li data-original-index="' + index + '"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">' + other + '</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
									$('#regionsList').append('<option value="' + IDs + '">' + other + '</option>');
								}
							});
						}
						else {
							other = regions[0].name.split("-")[0];
							IDs = regions[0].name.split("-")[1];

							$('#selectRegionsOuther').find('ul').append('<li data-original-index="0" class="selected"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">Nothing selected</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
							$('#selectRegionsOuther').find('ul').append('<li data-original-index="' + 1 + '"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">' + other + '</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');

							$('#regionsList').append('<option value="0">Nothing selected</option>');
							$('#regionsList').append('<option value="' + IDs + '">' + other + '</option>');
						}
					},
					error: function (ex) {
						console.log(ex)
					}
				});
				return false;
			}

			$("#countriesList").change(loadRegions);

			function loadCities() {
				$("#citiesList").empty();

				$('#selectCitiesOuther').find('ul').empty();
				$('#selectCitiesOuther').find('.filter-option').text('Nothing selected');
				$('#selectCitiesOuther').find('button').attr('title', 'Nothing selected');

				$.ajax({
					type: 'GET',
					url: '/Authentication/LoadCitiesForRegionAndCountry',
					dataType: 'json',
					data: { regionId: $('select[name="RegionId"]').val(), countryId: $('select[name="CountryId"]').val() },
					success: function (cities) {
						$('#selectCitiesOuther').find('ul').append('<li data-original-index="0" class="selected"><a tabindex="0" class="" data-tokens="null" role="option" aria-disabled="false" aria-selected="true"><span class="text">Nothing selected</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
						$("#citiesList").append('<option value="0">Nothing selected</option>');

						$.each(cities, function (i, city) {
							index = i + 1;
							$('#selectCitiesOuther').find('ul').append('<li data-original-index="' + index + '"><a tabindex="0" class="" data-tokens="null" role="option" ari-disabled="false" aria-selected="false"><span class="text">' + city.name + '</span><span class="glyphicon glyphicon-ok check-mark"></span></a></li>');
							$("#citiesList").append('<option value="' + city.id + '">' + city.name + '</option>');
						});
					},
					error: function (ex) {
						console.log(ex)
					}
				});
				return false;
			}

			$("#regionsList").change(loadCities);
		});
    </script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
